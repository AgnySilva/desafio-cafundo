
#services .container .row .footer{
	padding:20px;
	color: #fff;
}
.menuOP{
	margin-top:60px;
	margin-bottom:25px;
}

.social-icons ul{
	text-align: center;
	padding-left: 0;
	display: inline-block;
}
.social-icons ul a li{
	display: inline;
	list-style-type: none;
    border: 2.5px solid #ccc;
    margin-right: 0;
	padding: auto;
    background-color: #23468c;
    color: #fff;
	font-size: 105%;
    line-height: 30px;
    width: 36px;
	border-radius: 50%;
}
.social-icons ul a li:last-child{
	margin-right: 0;
}
.social-icons ul a:hover li{
	color: #23468c;
	background-color: #fff;
	border: 2px solid #23468c;
}
::title{
	background: #a94448;
	color: #fff;
	padding: 10px;
}
#menu2{
	text-transform: UPERCASE;
	margin: 0 60px 0 0;
}

